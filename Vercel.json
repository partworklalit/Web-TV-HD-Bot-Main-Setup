{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/telegram.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/index",
      "dest": "api/index.py"
    },
    {
      "src": "/api/telegram",
      "dest": "api/telegram.py"
    }
  ]
}
